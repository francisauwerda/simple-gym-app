{
  "expo": {
    "name": "Simple gym app",
    "slug": "simple-gym-app",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.1.14",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.simplegymapp.simplegymapp"
    },
    "android": {
      "package": "com.simplegymapp.simplegymapp",
      "versionCode": 15,
      "permissions": []
    }
  }
}
